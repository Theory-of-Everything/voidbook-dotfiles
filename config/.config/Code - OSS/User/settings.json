{
    "workbench.colorTheme": "Grunge Contrast (rainglow)",
    "workbench.editor.decorations.colors": false,
    "window.zoomLevel": -1,

    "workbench.colorCustomizations": {
        "statusBar.noFolderBackground": "#333",
        "statusBar.noFolderForeground": "#fff",
        "gitDecoration.addedResourceForeground": "#aaf2aa",
        "gitDecoration.untrackedResourceForeground": "#68c768",
        "gitDecoration.stageModifiedResourceForeground": "#aaf2aa",
        "gitDecoration.modifiedResourceForeground": "#5ac3ef",
        "gitDecoration.conflictingResourceForeground": "#ce3c65",
        "gitDecoration.deletedResourceForeground": "#f56991"
    },
    "editor.fontFamily": "'agave Nerd Font Mono'",
    "editor.fontSize": 16,

    "terminal.integrated.env.linux": {
        "visual_studio": "true"
    },
    "editor.suggestSelection": "first",
    "explorer.confirmDragAndDrop": false,
    "gitlens.defaultDateFormat": null,
    "gitlens.defaultDateShortFormat": null
}